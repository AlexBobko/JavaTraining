<?xml version="1.0" encoding="UTF-8"?>
<flowers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<flower>
		<name>Самоцветик</name>
		<soil>подзолистая</soil>
		<origin>Гондурас</origin>
		<visualParameters>
			<stemColor>Зеленый</stemColor>
			<stemSize>Маленький</stemSize>
		</visualParameters>
		<growingTips>
			<temperature>33</temperature>
			<watering>500</watering>
		</growingTips>
		<multiplying>черенками</multiplying>
	</flower>
	<flower>
		<name>Цветик</name>
		<soil>грунтовая</soil>
		<origin>Германия</origin>
		<visualParameters>
			<stemColor>Синий</stemColor>
			<stemSize>Большой</stemSize>
		</visualParameters>
		<growingTips>
			<temperature>22</temperature>
			<watering>400</watering>
		</growingTips>
		<multiplying>семенами</multiplying>
	</flower>
	<flower>
		<name>Перецветик</name>
		<soil>дерново-подзолистая</soil>
		<origin>Эфиопия</origin>
		<visualParameters>
			<stemColor>Красный</stemColor>
			<stemSize>Средний</stemSize>
		</visualParameters>
		<growingTips>
			<temperature>28</temperature>
			<watering>450</watering>
		</growingTips>
		<multiplying>черенками</multiplying>
	</flower>
</flowers>